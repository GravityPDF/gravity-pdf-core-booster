{
  "name": "gravity-pdf/core-booster",
  "type": "project",
  "license": "GPLv2",
  "require": {
    "php": ">=5.4"
  },
  "config": {
    "preferred-install": "dist",
    "autoloader-suffix": "GravityPDFCoreBoosterAddon"
  },
  "require": {
    "claviska/SimpleImage": "~2.7"
  },
  "require-dev": {
        "phpunit/phpunit": "~4.0",
        "adlawson/vfs": "^0.12.1"
  },
  "autoload": {
    "psr-4": {
      "GFPDF\\Plugins\\CoreBooster\\": "src/"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "yarn global add gulp-cli",
      "yarn && gulp"
    ]
  }
}
